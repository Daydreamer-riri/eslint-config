{
  "name": "@ririd/eslint-config",
  "type": "module",
  "version": "1.3.4",
  "packageManager": "pnpm@9.12.3",
  "author": "Daydreamer Riri <Daydreamerriri@outloot.com> (https://github.com/Daydreamer-riri/)",
  "license": "MIT",
  "homepage": "https://github.com/daydreamer-riri/eslint-config",
  "keywords": [
    "eslint-config"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --clean --dts",
    "stub": "tsup src/index.ts --format esm,cjs --dts --watch",
    "lint": "eslint .",
    "test": "pnpm -r run test",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp && pnpm publish",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "eslint": ">=8.0.0"
  },
  "dependencies": {
    "@antfu/eslint-config": "^3.8.0",
    "@eslint-react/eslint-plugin": "^1.15.2",
    "@next/eslint-plugin-next": "^14.2.17",
    "eslint-plugin-format": "^0.1.2",
    "eslint-plugin-react-hooks": "5.1.0-rc-06d0b89e-20240801",
    "eslint-plugin-react-refresh": "^0.4.14"
  },
  "devDependencies": {
    "@ririd/eslint-config": "workspace:*",
    "@types/eslint": "^9.6.1",
    "@types/node": "20.10.4",
    "bumpp": "^9.8.1",
    "eslint": "^9.14.0",
    "rimraf": "^6.0.1",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3",
    "vitest": "^2.1.4"
  }
}
