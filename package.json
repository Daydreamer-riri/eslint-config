{
  "name": "@ririd/eslint-config",
  "type": "module",
  "version": "1.3.2",
  "packageManager": "pnpm@9.4.0",
  "author": "Daydreamer Riri <Daydreamerriri@outloot.com> (https://github.com/Daydreamer-riri/)",
  "license": "MIT",
  "homepage": "https://github.com/daydreamer-riri/eslint-config",
  "keywords": [
    "eslint-config"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --clean --dts",
    "stub": "tsup src/index.ts --format esm,cjs --dts --watch",
    "lint": "eslint .",
    "test": "pnpm -r run test",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp && pnpm publish",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "eslint": ">=8.0.0"
  },
  "dependencies": {
    "@antfu/eslint-config": "^2.21.2",
    "@eslint-react/eslint-plugin": "^1.5.16",
    "@next/eslint-plugin-next": "^14.2.4",
    "eslint-plugin-format": "^0.1.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7"
  },
  "devDependencies": {
    "@ririd/eslint-config": "workspace:*",
    "@types/eslint": "^8.56.9",
    "@types/node": "20.10.4",
    "bumpp": "^9.4.0",
    "eslint": "^9.5.0",
    "rimraf": "^5.0.5",
    "tsup": "^8.1.0",
    "typescript": "^5.5.2",
    "vitest": "^1.5.2"
  }
}
