{
  "name": "@ririd/eslint-config",
  "type": "module",
  "version": "2.0.0",
  "packageManager": "pnpm@10.16.1",
  "author": "Daydreamer Riri <Daydreamerriri@outloot.com> (https://github.com/Daydreamer-riri/)",
  "license": "MIT",
  "homepage": "https://github.com/daydreamer-riri/eslint-config",
  "keywords": [
    "eslint-config"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --clean --dts",
    "stub": "tsup src/index.ts --format esm,cjs --dts --watch",
    "lint": "eslint .",
    "test": "pnpm -r run test",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp && pnpm publish",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "eslint": ">=8.0.0"
  },
  "dependencies": {
    "@antfu/eslint-config": "^5.3.0",
    "@eslint-react/eslint-plugin": "^1.53.1",
    "@next/eslint-plugin-next": "^15.5.3",
    "eslint-plugin-format": "^1.0.1",
    "eslint-plugin-react-hooks": "5.1.0",
    "eslint-plugin-react-refresh": "^0.4.20"
  },
  "devDependencies": {
    "@ririd/eslint-config": "workspace:*",
    "@types/eslint": "^9.6.1",
    "@types/node": "20.10.4",
    "bumpp": "^10.2.3",
    "eslint": "^9.35.0",
    "rimraf": "^6.0.1",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
